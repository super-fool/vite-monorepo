{
  "extends": [
    "@commitlint/config-angular"
  ],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "chore",
        "docs",
        "feat",
        "fix",
        "refactor",
        "style"
      ]
    ],
    "header-case": [
      2,
      "always",
      [
        "lower-case"
      ]
    ]
  }
}